{
  "name": "@aemforms/af-react-mui-components",
  "version": "1.0.0",
  "description": "Material UI based React Components for Headless Forms",
  "author": "Adobe Systems",
  "license": "Adobe Proprietary",
  "homepage": "",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "jest-junit": {
    "suiteName": "component tests",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "component-test.{filename}",
    "outputDirectory": "./target/test-reports"
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "publishConfig": {
