    "@mui/material": "^5.13.5",
    "@mui/x-date-pickers": "^6.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.8.4",
    "sanitize-html": "^2.10.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.11",
